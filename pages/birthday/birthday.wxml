<!--pages/birthday/birthday.wxml-->
<view class="page" data-weui-theme="light">
  <view>
    <view class=' list-item-icon'><image src="/Icon/happybirthday.png"></image></view>
    <view class='list-item-title'>happybirthday</view>
  </view>
  <view wx:for="{{wish}}">
    <view class="weui-cell_access weui-cell">{{item.content}}</view>
  </view>
  
  <view bindtap="updata" class="weui-footer weui-footer_fixed-bottom">
    <image class="wmui-update-img" src="/Icon/update.png"></image>
  </view>
</view>
